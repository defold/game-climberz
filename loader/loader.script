local monarch = require "monarch.monarch"

function init(self)
	math.randomseed(os.time()) math.random() math.random() math.random()

	msg.post("@render:/", "clear_color", { color = vmath.vector4(0xaa / 0xff, 0xea / 0xff, 1.0, 1.0) })
